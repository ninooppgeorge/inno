- var showcart = 0
- var showavatar = 1
doctype html
html(lang="en")
  include common/head
  body
    .body.contentmanagement.insider.orhis
      include common/nav
      // This is the content area
      // breadcrumbs
      .breadcrumbs
        .container
          .row
            ul
              li.home
                a(href='index.html', title='Go to Home Page') HOME
                span  ⁄ 
              li
                a(href='#', title='Go to Home Page') SPIFF CLAIM
      // End breadcrumbs
      // Two columns content
      .main-container.col2-left-layout
        .main.container
          .row
            aside.col-left.sidebar.col-sm-3(style='visibility: visible;')
              .block.block-account
                .block-title
                  a(href='my_account.html')
                    i.fa.fa-lock(style=' color: #888;')
                    |    My Account
                .block-content
                  ul
                    li
                      a(href='#') Account Settings
                    li
                      a(href='#') Contact Details
                .block-title
                  a(href='rebatecenter.html')
                    i.fa.fa-tasks(style=' color: #888;')
                    |    Rebate Center
                .block-title
                  a(href='spiffclaim.html')
                    i.fa.fa-tasks(style=' color: #888;')
                    |    SPIFF CLAIM
            section.col-main.col-sm-9(style='background: #f0f0f0; padding-top: 15px;')
              .category-products
                .toolbar(style='padding-top:0px')
                  .content
                    .page-title
                      h2(style=' border-bottom:1px solid #f1f1f1; padding-bottom:10px; margin-bottom:10px') NEW CLAIM ENTRY
                    .group-select
                      ul
                        li
                          .seriel-number
                            .input-box(style='width:100%')
                              label(for='spiff:serial')
                                | Enter Serial Number
                                span.required *
                              br
                              input#serialnum.input-text(type='text', placeholder='Serial Number', name='serialnum', value='', title='Serial Num')
                        p.claimmsg Enter the serial number of the unit you want to claim the SPIFF on
                        .buttons-set
                          button.button.submit.margin-top--0(type='submit', title='Submit')
                            span  Validate 
                    br
                    br
                    p(style='font-size:13px;')
                      | Upon submission, C.E.D Inc will validate the claim and payment will be made within 2 to 4 weeks from submission date.
                      | You may check the status of the claim at anytime by signing into C.E.D Inc.
                    table#myTable.tablesorter
                      thead
                        tr
                          th.header DATA
                          th.header DESCRIPTION
                          th.header STATUS
                      tbody
                        tr.odd
                          td No Data
                          td
                          td
      // This is the content area
    include common/footer
    include common/scripts
    link(rel='stylesheet', href='../content/jquery.tablesorter.pager.css')
    script(src='../scripts/jquery.tablesorter.min.js')
    script(src='../scripts/jquery.tablesorter.pager.js')
